{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/mail": "<rootDir>/../libs/mail/src",
    "@app/mail/(.*)": "<rootDir>/../libs/mail/src/$1",
    "@app/minio": "<rootDir>/../libs/minio/src",
    "@app/minio/(.*)": "<rootDir>/../libs/minio/src/$1"
  }
}